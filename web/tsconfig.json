{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
  ],
  "exclude": [
    "src/**/__tests__/*",
    "node_modules",
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "module": "ESNext",
    "types": [
      "element-plus/global"
    ],
    "target": "ESNext",
  }
}